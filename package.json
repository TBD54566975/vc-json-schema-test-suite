{
  "name": "vc-json-schema-test-suite",
  "version": "1.0.0",
  "description": "W3C Verifiable Credential JSON Schema test suite.",
  "homepage": "https://github.com/w3c/vc-json-schema-test-suite",
  "author": {
    "name": "W3C Verifiable Credentials Working Group",
    "email": "public-vc-wg@w3.org",
    "url": "https://www.w3.org/groups/wg/vc/"
  },
  "main": "/tests",
  "type": "module",
  "directories": {
    "tests": "tests"
  },
  "files": [
    "implementations/*",
    "tests/*.js"
  ],
  "scripts": {
    "test": "mocha tests/ --reporter @digitalbazaar/mocha-w3c-interop-reporter --reporter-options abstract=\"$PWD/abstract.hbs\",reportDir=\"$PWD/reports\",respec=\"$PWD/respecConfig.json\",title=\"VC JSON Schema Test Suite\",suiteLog='./suite.log' --timeout 15000 --preserve-symlinks",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3c/vc-json-schema-test-suite.git"
  },
  "bugs": {
    "url": "https://github.com/w3c/vc-json-schema-test-suite/issues"
  },
  "dependencies": {
    "@digitalbazaar/did-method-key": "^5.1.0",
    "@digitalbazaar/ecdsa-multikey": "^1.1.3",
    "@digitalbazaar/http-client": "^3.4.1",
    "@digitalbazaar/mocha-w3c-interop-reporter": "^1.4.0",
    "@yimura/import-dir": "^0.1.12",
    "app-root-path": "^3.1.0",
    "base58-universal": "^2.0.0",
    "chai": "^4.3.7",
    "data-integrity-test-suite-assertion": "github:w3c-ccg/data-integrity-test-suite-assertion",
    "jsonld-document-loader": "^2.0.0",
    "klona": "^2.0.6",
    "mocha": "^10.2.0",
    "require-dir": "^1.2.0",
    "shelljs": "^0.8.5",
    "uuid": "^9.0.0",
    "vc-test-suite-implementations": "github:w3c/vc-test-suite-implementations"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-digitalbazaar": "^5.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^47.0.0"
  }
}
